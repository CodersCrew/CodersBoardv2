{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "check:lint": "eslint \"*/**/*.{js,ts,tsx}\"",
    "check:types": "tsc --noEmit",
    "check-all": "yarn check:lint && yarn check:types",
    "generate:translations": "ts-node --skip-project ./src/generators/translations.ts && eslint \"./src/generated/**/*.{tsx,ts}\" --fix",
    "generate:svg": "npx @svgr/cli --ext tsx --no-dimensions -d src/components/svg src/assets/svg && eslint \"./src/components/svg/*.{tsx,ts}\" --fix",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reduxjs/toolkit": "^1.3.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "antd": "^4.2.0",
    "axios": "^0.21.1",
    "clsx": "^1.1.0",
    "emotion-theming": "^10.0.27",
    "formik": "^2.1.4",
    "formik-antd": "^2.0.1",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "i18next-xhr-backend": "^3.2.2",
    "polished": "^3.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-use": "^14.2.0",
    "redux-saga": "^1.1.3",
    "styled-system": "^5.1.5",
    "typescript": "~3.8.3",
    "utility-types": "^3.10.0",
    "yup": "^0.28.4"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@svgr/cli": "^5.4.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-system": "^5.1.9",
    "@types/styled-system__should-forward-prop": "^5.1.1",
    "@types/yup": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-plugin-emotion": "^10.0.33",
    "craco-alias": "^2.1.1",
    "craco-antd": "^1.14.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  }
}
