{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@coders-board-library/event-sourcing/(.*)": "<rootDir>/../libs/event-sourcing/src/$1",
    "@coders-board-library/event-sourcing": "<rootDir>/../libs/event-sourcing/src",
    "@coders-board-library/time-provider/(.*)": "<rootDir>/../libs/time-provider/src/$1",
    "@coders-board-library/time-provider": "<rootDir>/../libs/time-provider/src",
    "@coders-board-in-memory-transport/in-memory-transport/(.*)": "<rootDir>/../libs/in-memory-transport/src/$1",
    "@coders-board-in-memory-transport/in-memory-transport": "<rootDir>/../libs/in-memory-transport/src"
  }
}